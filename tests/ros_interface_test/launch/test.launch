<launch>
    <!-- <node pkg="tf" type="static_transform_publisher" name="camera_tf_broadcaster" args="0 0 0 0 0 0 base_link camera_link 50" /> -->
    <!-- <node pkg="tf" type="static_transform_publisher" name="camera_tf_broadcaster" args="0 0 0 0.707107 0 0 0.707107 base_link camera_link 50" /> -->

    <node pkg="mems_complementary_filter_ros" type="ros_interface_test" name="ros_interface_test" output="screen" respawn="false" >
        <param name="do_bias_estimation"         type="bool"    value="true"/>
        <param name="do_adaptive_gain"           type="bool"    value="true"/>
        <param name="use_magnetometer"           type="bool"    value="false"/>
        <param name="output_relative_yaw"        type="bool"    value="false"/>
        <param name="supervise_states"           type="bool"    value="true"/>
        <param name="publish_tf"                 type="bool"    value="false"/>
        <param name="transform_x_axisd"          type="double"  value="0.0"/>
        <param name="transform_z_axisd"          type="double"  value="0.0"/>
        
        <remap from="imu/data_raw"              to="/wit/imu"/>
        <remap from="imu/mag"                   to="/wit/mag"/>
    </node>

</launch>